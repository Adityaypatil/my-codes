#include<stdio.h>


void main()
{
int Q, i,j,arr[]={3,6,7,4,9,8,12 };
//length=
printf("Enter differnce:");
scanf("%d",&Q);
diff(arr,Q);

}

int diff(int *arr,int Q)
{
int i,j;
for(i=0;i<7;i++)
	{
	for(j=i+1;j<7;j++)
	{
		if(arr[i] - arr[j]==Q || arr[j] - arr[i]==Q)
			{
			printf("[%d %d]",arr[i],arr[j]);
			}
	}
	}
return;
}
	
