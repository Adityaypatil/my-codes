struct node
{
  int data;
  struct node *left=NULL;
  struct node *right=NULL; 
}*root=NULL;



struct node *create(int data)
{
  struct node *newnode=(struct node *)malloc(sizeof(struct node));

    newnode->data=data;
    newnode->left=NULL:
    newnode->right=NULL;

  return newnode;
}




