#include<stdio.h>

int rec_bin(int arr[],int low,int heigh,int size)
{
  int mid=low+(heigh-low)/2;
  if(arr[mid]==data)
    return mid;
  else if(arr[mid] < data)
    return rec_bin(arr,low,mid+1,size);
  else return rec_bin(arr,mid-1,heigh,size);
}


int main()
{
  int arr[]={1,2,3,4,5,6};
  int size = sizeof(arr)/sizeof(arr[0]);

  printf("==>%d",rec_bin(arr,low,heigh,size));
  return 0;
}
